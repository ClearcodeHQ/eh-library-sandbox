{
  "name": "clearcode/eh-library-sandbox",
  "description": "Sandbox for check how eh-library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Clearcode",
      "homepage": "http://clearcode.cc/"
    }
  ],
  "autoload": {
    "psr-4" : { "Clearcode\\EHLibrarySandbox\\": "src" }
  },
  "autoload-dev": {
    "psr-4" : {
      "tests\\Clearcode\\EHLibrarySandbox\\" : "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5",
    "silex/silex": "~1.0",
    "symfony/class-loader": "~2.3",
    "symfony/config": "~2.3",
    "symfony/console": "~2.3",
    "symfony/debug": "~2.3",
    "symfony/finder": "~2.3",
    "symfony/process": "~2.3",
    "symfony/monolog-bridge": "~2.3",
    "clearcode/eh-library": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "symfony/browser-kit": "~2.3",
    "symfony/css-selector": "~2.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ClearcodeHQ/eh-library"
    }
  ]
}
